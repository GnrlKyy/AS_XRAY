            sslnetwork["Payload"] = AESCrypt.decrypt(password, sslnetwork["Payload"])
            sslnetwork["ProxySettings"]["Squid"] = AESCrypt.decrypt(password, sslnetwork["ProxySettings"]["Squid"])

        encrypted_data["Username"] = AESCrypt.decrypt(password, encrypted_data["Username"])
        encrypted_data["Password"] = AESCrypt.decrypt(password, encrypted_data["Password"])
        encrypted_data["CloudfrontDNS"] = AESCrypt.decrypt(password, encrypted_data["CloudfrontDNS"])


