                server["V2Ray"]["V2RayConfig"] = AESCrypt.decrypt(password, server["V2Ray"]["V2RayConfig"])

        for network in encrypted_data["Networks"]:
            network["Payload"] = AESCrypt.decrypt(password, network["Payload"])
            network["ProxySettings"]["Squid"] = AESCrypt.decrypt(password, network["ProxySettings"]["Squid"])   
            if "DNSResolver" in network:
                network["DNSResolver"] = AESCrypt.decrypt(password, network["DNSResolver"])

            if "SNIHost" in network:
                network["SNIHost"] = AESCrypt.decrypt(password, network["SNIHost"])

