        decrypted_text = decrypt(decrypted_content)
        encrypted_data = json.loads(decrypted_text)

        for server in encrypted_data["Servers"]:
            server["ServerIPHost"] = AESCrypt.decrypt(password, server["ServerIPHost"])
            server["CloudfrontDNS"] = AESCrypt.decrypt(password, server["CloudfrontDNS"])
            server["ServerHTTP"] = AESCrypt.decrypt(password, server["ServerHTTP"])
            server["Username"] = AESCrypt.decrypt(password, server["Username"])
            server["Password"] = AESCrypt.decrypt(password, server["Password"])
